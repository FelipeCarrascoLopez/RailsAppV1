<h1>Profiles</h1>
<% if current_user.profile.admin? %>

<div id="profiles">
  <% @profiles.each do |profile| %>
    <%= render profile %>
    <p>
      <%= link_to "Info Usuario", profile %>
    </p>
  <% end %>
</div>

<% else %>

<div id="profiles">
    <%= render current_user.profile %>
</div>

<% end %>

